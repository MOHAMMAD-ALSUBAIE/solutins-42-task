<html>
 <head>
    <style>
        @import url('https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,700');



*, *:before, *:after {
	box-sizing:border-box;
}

body {
	font-family:'Source Sans Pro', sans-serif;
	margin:0;
}
a{
	text-align: center;
	text-decoration: none;
	color: black;
	background-color: #5b8024;
        color: white;
        padding: 14px 0;
        margin: 10px 0;
        border-radius: 0.5rem;
		border: none;
        cursor: pointer;
        width: 7rem;
		font-size: larger;
}


	  .buttonContainer{
	display: flex;
	justify-content: start;
}

.container {
	max-width: 1000px;
	margin-right:auto;
	margin-left:auto;
	display:flex;
	align-items:start;

	flex-direction: column;
	margin-top: 15%;
	
}

.table {
	width:100%;
	border:1px solid #EEEEEE;
}

.table-header {
    padding:24px ;
	display:flex;
	width:100%;
	background:#5b8024;
	padding:(12px * 1.5) 0;
}

.table-row {
	display:flex;
	
	width:100%;
	padding:(12px * 1.5) 0;
	

}

.table-data, .header__item {
	flex: 1 1 20%;
	text-align:center;
}

.header__item {
    color:white;

}

    </style>
 </head>  
 <body>
<div class="container">
	
	<div class="table">
		<div class="table-header">
        <%Object.keys(data).forEach((item)=> { %>
            <% if(item == 'Time') {%>
			<div class="header__item"><%=item %> operation </div>
		    
              <%}else{%>
                <div class="header__item"><%=item %>   </div>
                
            <% }})%>         
		</div>
		<div class="table-content">	
			<div class="table-row">	
                <%Object.values(data).forEach((item)=> { %>
                 <% if(typeof item === 'string' &&  item.split(' ').includes("seconds")){%>
                    <div class="table-data"><%=item %></div>

                  <%}else{%>
                    <div class="table-data"><%=item %> records </div>
                    <%}%>
                <%})%>    	
				
			
			</div>
		</div>	
	</div>
	<div class="buttonContainer">
		<a href="/">Home</a>
		
	</div>
</div>
</body> 
</html>